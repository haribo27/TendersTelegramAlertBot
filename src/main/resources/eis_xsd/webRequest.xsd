    <xs:element name="publishedDataRequest">
        <xs:annotation>
            <xs:documentation>Запрос от ВСРЗ на выгрузку данных</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="t:packetType">
                    <xs:sequence>
                        <xs:element name="body">
                            <xs:annotation>
                                <xs:documentation>Данные информационного пакета</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:choice>
                                        <xs:element name="guidList">
                                            <xs:annotation>
                                                <xs:documentation>Перечень GUID</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="guid" type="t:guidType" minOccurs="1" maxOccurs="1000">
                                                        <xs:annotation>
                                                            <xs:documentation>GUID сведений для поиска и выгрузки</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>

                                        <xs:element name="electronicPlaceId" type="xs:long">
                                            <xs:annotation>
                                                <xs:documentation>Идентификатор позиции справочника</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                    </xs:choice>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

