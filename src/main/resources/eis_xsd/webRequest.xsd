    <xs:complexType name="ItemTypeWebRequest">
        <xs:annotation>
            <xs:documentation>Описание элемента ответа</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="guid" type="t:guidType">
                <xs:annotation>
                    <xs:documentation>GUID сведений</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="entityName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Вид сведений</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="purchaseNotice">
                            <xs:annotation>
                                <xs:documentation>Извещение о закупке</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="purchasePlan">
                            <xs:annotation>
